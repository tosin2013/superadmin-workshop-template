---
controller_templates:
  - name: "{{ organization_name }} {% raw %}{{ env }}{% endraw %} CasC Ctrl CD Webhook Trigger"
    description: "Template to attend Controller CasC webhook"
    organization: "{{ orgs }}"
    project: "{{ orgs }} {% raw %}{{ env }}{% endraw %} CasC Data"
    inventory: "{{ orgs }} {% raw %}{{ env }}{% endraw %} Controller"
    playbook: "casc_ctrl_cd_webhook_trigger.yml"
    job_type: run
    fact_caching_enabled: false
    credentials:
      - "{{ orgs }} {% raw %}{{ env }}{% endraw %} AAP Credential"
      - "{{ orgs }} {% raw %}{{ env }}{% endraw %} Vault Credential"
    webhook_service: "gitlab"
    concurrent_jobs_enabled: true
    ask_scm_branch_on_launch: true
    extra_vars:
      ansible_python_interpreter: /usr/bin/python3
      ansible_async_dir: /home/runner/.ansible_async/
    execution_environment: "ee-casc"
  - name: "{{ organization_name }} {% raw %}{{ env }}{% endraw %} CasC Ctrl Config"
    description: "Template to attend Controller CasC Configuration"
    organization: "{{ orgs }}"
    project: "{{ orgs }} {% raw %}{{ env }}{% endraw %} CasC Data"
    inventory: "{{ orgs }} {% raw %}{{ env }}{% endraw %} Controller"
    playbook: "casc_ctrl_config.yml"
    job_type: run
    fact_caching_enabled: false
    credentials:
      - "{{ organization_name }} {% raw %}{{ env }}{% endraw %} AAP CasC Credential"
      - "{{ orgs }} {% raw %}{{ env }}{% endraw %} Vault Credential"
      - "{{ orgs }} {% raw %}{{ env }}{% endraw %} GitLab API Token"
    concurrent_jobs_enabled: true
    ask_scm_branch_on_launch: true
    ask_tags_on_launch: true
    ask_verbosity_on_launch: true
    ask_variables_on_launch: true
    extra_vars:
      ansible_python_interpreter: /usr/bin/python3
      ansible_async_dir: /home/runner/.ansible_async/
    execution_environment: "ee-casc"
  - name: "{{ organization_name }} {% raw %}{{ env }}{% endraw %} CasC Ctrl Drop Diff"
    description: "Template to attend Controller CasC Drop Diff"
    organization: "{{ orgs }}"
    project: "{{ orgs }} {% raw %}{{ env }}{% endraw %} CasC Data"
    inventory: "{{ orgs }} {% raw %}{{ env }}{% endraw %} Controller"
    playbook: "casc_ctrl_drop_diff.yml"
    job_type: run
    fact_caching_enabled: false
    credentials:
      - "{{ organization_name }} {% raw %}{{ env }}{% endraw %} AAP CasC Credential"
      - "{{ orgs }} {% raw %}{{ env }}{% endraw %} Vault Credential"
      - "{{ orgs }} {% raw %}{{ env }}{% endraw %} GitLab API Token"
    webhook_service: "gitlab"
    concurrent_jobs_enabled: true
    ask_scm_branch_on_launch: true
    ask_tags_on_launch: true
    ask_variables_on_launch: true
    extra_vars:
      ansible_python_interpreter: /usr/bin/python3
      ansible_async_dir: /home/runner/.ansible_async/
    execution_environment: "ee-casc"
...
